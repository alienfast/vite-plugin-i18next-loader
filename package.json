{
  "name": "vite-plugin-i18next-loader",
  "version": "0.0.1",
  "description": "Vite plugin loader for client embedded i18next locales from json or yaml.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup-node",
    "build:ide": "echo 'tsc -b' && tsc -b",
    "clean": "echo 'cleaning' && tsc -b --clean && rm -rf packages/*/dist || true && rimraf .eslintcache || true",
    "clean:yarn": "rimraf yarn.lock node_modules .yarn/install-state.gz && yarn cache clean",
    "lint": "eslint . --cache --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --cache --ext .js,.ts,.tsx --fix",
    "test": "yarn test:unit",
    "test:unit": "vitest"
  },
  "keywords": [
    "vite-plugin",
    "loader",
    "i18next",
    "locales",
    "yaml",
    "json"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alienfast/i18next-loader"
  },
  "homepage": "https://github.com/alienfast/i18next-loader",
  "maintainers": [
    {
      "name": "Kevin Ross",
      "email": "kevin.ross@alienfast.com"
    }
  ],
  "peerDependencies": {
    "vite": ">=3.1.6"
  },
  "dependencies": {
    "glob-all": "^3.1.0",
    "js-yaml": "^3.13.1",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^4.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.8.3",
    "@types/rimraf": "^3",
    "eslint": "^8.24.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vitest": "^0.23.4"
  }
}
