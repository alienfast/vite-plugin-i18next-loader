  "extends": "@alienfast/tsconfig/node.json",

  "include": ["src", "typings/**/*"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "rootDir": "src"
  }
}
